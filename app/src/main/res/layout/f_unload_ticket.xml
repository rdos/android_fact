<?xml version="1.0" encoding="utf-8"?>
<ru.smartro.worknote.ac.SmartROllc
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent" android:layout_height="wrap_content"
	android:orientation="vertical"
	android:paddingVertical="8dp"
	android:paddingHorizontal="12dp"
	android:layout_margin="0dp"
	android:background="@drawable/bg_button_white__rounded"
	>

	<androidx.appcompat.widget.AppCompatTextView
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:text="Выгрузка"
		android:textColor="@color/black"
		android:textStyle="bold"
		android:textSize="16sp"
		android:layout_gravity="center"
	/>

	<androidx.constraintlayout.widget.ConstraintLayout
		android:layout_width="match_parent" android:layout_height="wrap_content"
		android:layout_marginTop="12dp"
	>

		<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/actv__f_unload_ticket__photo_before"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Фото весов до"
			app:layout_constraintBottom_toBottomOf="@+id/llc__f_unload_ticket__photo_before"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="@+id/llc__f_unload_ticket__photo_before"
			/>

		<androidx.appcompat.widget.LinearLayoutCompat
			android:id="@+id/llc__f_unload_ticket__photo_before"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:orientation="horizontal"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			>
			<ru.smartro.worknote.ac.SmartROacb
				android:id="@+id/acb__f_unload_ticket__photo_before"
				android:layout_width="wrap_content"
				android:layout_height="40dp"
				android:layout_marginEnd="8dp"
				android:backgroundTint="@color/colorPrimary"
				android:minWidth="0dp"
				android:minHeight="0dp"
				android:paddingHorizontal="10dp"
				android:paddingVertical="6dp"
				android:drawableEnd="@drawable/ic_add_photo"
				android:textColor="@color/white"
				android:textSize="16sp"

				/>
			<androidx.appcompat.widget.AppCompatTextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginEnd="4dp"
				android:text="/"
				android:textColor="@color/black"
				android:textSize="16sp"/>

			<androidx.appcompat.widget.AppCompatTextView
				android:id="@+id/actv__f_unload_ticket__photo_before_count"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="@color/black"
				android:textSize="16sp"
				/>

		</androidx.appcompat.widget.LinearLayoutCompat>




		<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/actv__f_unload_ticket__value_before"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Значение на весах до"
			app:layout_constraintTop_toTopOf="@+id/acet__f_unload_ticket__value_before"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintBottom_toBottomOf="@+id/acet__f_unload_ticket__value_before"
			/>

		<androidx.appcompat.widget.AppCompatEditText
			android:id="@+id/acet__f_unload_ticket__value_before"
			android:layout_width="100dp" android:layout_height="wrap_content"
			android:paddingVertical="6dp"
			android:paddingHorizontal="12dp"
			android:maxWidth="120dp"
			android:maxLines="1"
			android:inputType="numberDecimal|numberSigned"
			android:layout_marginTop="12dp"
			app:layout_constraintTop_toBottomOf="@id/llc__f_unload_ticket__photo_before"
			app:layout_constraintEnd_toEndOf="parent"
			/>



		<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/actv__f_unload_ticket__photo_after"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Фото весов после"
			app:layout_constraintBottom_toBottomOf="@+id/llc__f_unload_ticket__photo_after"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="@+id/llc__f_unload_ticket__photo_after"
			/>


		<androidx.appcompat.widget.LinearLayoutCompat
			android:id="@+id/llc__f_unload_ticket__photo_after"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:orientation="horizontal"
			app:layout_constraintTop_toBottomOf="@id/acet__f_unload_ticket__value_before"
			app:layout_constraintEnd_toEndOf="parent"
			>
			<ru.smartro.worknote.ac.SmartROacb
				android:id="@+id/acb__f_unload_ticket__photo_after"
				android:layout_width="wrap_content"
				android:layout_height="40dp"
				android:layout_marginEnd="8dp"
				android:backgroundTint="@color/colorPrimary"
				android:minWidth="0dp"
				android:minHeight="0dp"
				android:paddingHorizontal="10dp"
				android:paddingVertical="6dp"
				android:drawableEnd="@drawable/ic_add_photo"
				android:textColor="@color/white"
				android:textSize="16sp"

				/>
			<androidx.appcompat.widget.AppCompatTextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginEnd="4dp"
				android:text="/"
				android:textColor="@color/black"
				android:textSize="16sp"/>

			<androidx.appcompat.widget.AppCompatTextView
				android:id="@+id/actv__f_unload_ticket__photo_after_count"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="@color/black"
				android:textSize="16sp"
				/>

		</androidx.appcompat.widget.LinearLayoutCompat>


		<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/actv__f_unload_ticket__value_after"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Значение на весах после"
			app:layout_constraintTop_toTopOf="@+id/acet__f_unload_ticket__value_after"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintBottom_toBottomOf="@+id/acet__f_unload_ticket__value_after"
			/>

		<androidx.appcompat.widget.AppCompatEditText
			android:id="@+id/acet__f_unload_ticket__value_after"
			android:layout_width="100dp" android:layout_height="wrap_content"
			android:paddingVertical="6dp"
			android:paddingHorizontal="12dp"
			android:maxWidth="120dp"
			android:maxLines="1"
			android:inputType="numberDecimal|numberSigned"
			android:layout_marginTop="12dp"
			app:layout_constraintTop_toBottomOf="@id/llc__f_unload_ticket__photo_after"
			app:layout_constraintEnd_toEndOf="parent"
			/>

		<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/actv__f_unload_ticket__value_talon"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Значение в Талоне"
			app:layout_constraintTop_toTopOf="@+id/acet__f_unload_ticket__value_talon"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintBottom_toBottomOf="@+id/acet__f_unload_ticket__value_talon"
			/>

		<androidx.appcompat.widget.AppCompatEditText
			android:id="@+id/acet__f_unload_ticket__value_talon"
			android:layout_width="100dp" android:layout_height="wrap_content"
			android:paddingVertical="6dp"
			android:paddingHorizontal="12dp"
			android:maxWidth="120dp"
			android:maxLines="1"
			android:inputType="numberDecimal|numberSigned"
			android:layout_marginTop="12dp"
			app:layout_constraintTop_toBottomOf="@id/acet__f_unload_ticket__value_after"
			app:layout_constraintEnd_toEndOf="parent"
			/>

	</androidx.constraintlayout.widget.ConstraintLayout>

	<ru.smartro.worknote.ac.swipebtn.SmartROviewSwipeButton
		android:id="@+id/acb_f_unload_ticket__finish"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_marginTop="12dp"
		app:innerText="Проведите вправо,\nчтобы завершить"
		app:buttonSrc="@drawable/bg_button_orange"
		app:tailColor="@color/dark_orange"
		/>

</ru.smartro.worknote.ac.SmartROllc>