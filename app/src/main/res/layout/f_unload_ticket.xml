<?xml version="1.0" encoding="utf-8"?>
<ru.smartro.worknote.andPOintD.SmartROllc
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent" android:layout_height="wrap_content"
	android:orientation="vertical"
	android:padding="8dp"
	>

	<androidx.appcompat.widget.AppCompatTextView
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:text="Выгрузка"
		android:textColor="@color/black"
		android:textStyle="bold"
		android:textSize="16sp"
		android:layout_gravity="center"
	/>

	<androidx.constraintlayout.widget.ConstraintLayout
		android:layout_width="match_parent" android:layout_height="wrap_content"
		android:layout_marginTop="12dp"
	>

		<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/actv__f_unload_ticket__photo_before"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Фото весов до"
			app:layout_constraintBottom_toBottomOf="@+id/acib__f_unload_ticket__photo_before"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="@+id/acib__f_unload_ticket__photo_before" />

		<androidx.appcompat.widget.AppCompatImageButton
			android:id="@+id/acib__f_unload_ticket__photo_before"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:paddingVertical="6dp"
			android:paddingHorizontal="12dp"
			android:src="@drawable/ic_add_photo"
			android:background="@drawable/bg_button"
			android:foreground="?attr/selectableItemBackground"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
		/>

		<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/actv__f_unload_ticket__value_before"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Значение на весах до"
			app:layout_constraintTop_toTopOf="@+id/acet__f_unload_ticket__value_before"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintBottom_toBottomOf="@+id/acet__f_unload_ticket__value_before"
			/>

		<androidx.appcompat.widget.AppCompatEditText
			android:id="@+id/acet__f_unload_ticket__value_before"
			android:layout_width="100dp" android:layout_height="wrap_content"
			android:paddingVertical="6dp"
			android:paddingHorizontal="12dp"
			android:maxWidth="120dp"
			android:layout_marginTop="12dp"
			app:layout_constraintTop_toBottomOf="@id/acib__f_unload_ticket__photo_before"
			app:layout_constraintEnd_toEndOf="parent"
			/>



		<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/actv__f_unload_ticket__photo_after"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Фото весов после"
			app:layout_constraintBottom_toBottomOf="@+id/acib__f_unload_ticket__photo_after"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="@+id/acib__f_unload_ticket__photo_after" />

		<androidx.appcompat.widget.AppCompatImageButton
			android:id="@+id/acib__f_unload_ticket__photo_after"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:paddingVertical="6dp"
			android:paddingHorizontal="12dp"
			android:src="@drawable/ic_add_photo"
			android:background="@drawable/bg_button"
			android:foreground="?attr/selectableItemBackground"
			android:layout_marginTop="12dp"
			app:layout_constraintTop_toBottomOf="@id/acet__f_unload_ticket__value_before"
			app:layout_constraintEnd_toEndOf="parent"
			/>

		<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/actv__f_unload_ticket__value_after"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Значение на весах после"
			app:layout_constraintTop_toTopOf="@+id/acet__f_unload_ticket__value_after"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintBottom_toBottomOf="@+id/acet__f_unload_ticket__value_after"
			/>

		<androidx.appcompat.widget.AppCompatEditText
			android:id="@+id/acet__f_unload_ticket__value_after"
			android:layout_width="100dp" android:layout_height="wrap_content"
			android:paddingVertical="6dp"
			android:paddingHorizontal="12dp"
			android:maxWidth="120dp"
			android:layout_marginTop="12dp"
			app:layout_constraintTop_toBottomOf="@id/acib__f_unload_ticket__photo_after"
			app:layout_constraintEnd_toEndOf="parent"
			/>

		<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/actv__f_unload_ticket__value_talon"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Значение в Талоне"
			app:layout_constraintTop_toTopOf="@+id/acet__f_unload_ticket__value_talon"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintBottom_toBottomOf="@+id/acet__f_unload_ticket__value_talon"
			/>

		<androidx.appcompat.widget.AppCompatEditText
			android:id="@+id/acet__f_unload_ticket__value_talon"
			android:layout_width="100dp" android:layout_height="wrap_content"
			android:paddingVertical="6dp"
			android:paddingHorizontal="12dp"
			android:maxWidth="120dp"
			android:layout_marginTop="12dp"
			app:layout_constraintTop_toBottomOf="@id/acet__f_unload_ticket__value_after"
			app:layout_constraintEnd_toEndOf="parent"
			/>

	</androidx.constraintlayout.widget.ConstraintLayout>

	<ru.smartro.worknote.andPOintD.swipebtn.SmartROviewSwipeButton
		android:id="@+id/acb_f_unload_ticket__start"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_marginTop="12dp"
		app:innerText="Проведите вправо,\nчтобы завершить"
		app:buttonSrc="@drawable/bg_button_orange"
		/>

</ru.smartro.worknote.andPOintD.SmartROllc>