<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyExampleView">
        <attr name="tooltipNext" format="string" />
        <attr name="tooltipType" format="enum" >
            <enum name="ONCLICK" value="0"/>
            <enum name="ONROOT" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="sViewSwipeButton">
        <attr name="buttonIcon" format="reference" />
        <attr name="buttonSrc" format="reference" />
        <attr name="trailBackground" format="reference|color" />
        <attr name="innerText" format="reference|string" />
        <attr name="backgroundSrc" format="reference|color" />
    </declare-styleable>

</resources>